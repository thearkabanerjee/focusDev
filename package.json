  "version": "0.1.1",
  "icon": "assests/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/thearkabanerjee/focusdev"
  },
  "engines": {
    "vscode": "^1.50.0"
  },
  "keywords": [
    "pomodoro", "timer", "productivity", "focus", "work", "break", "long break",
    "pomodoro technique", "pomodoro timer", "pomodoro productivity", "pomodoro productivity timer",
    "pomodoro productivity extension", "pomodoro productivity app",
    "pomodoro productivity tool", "pomodoro productivity software", "pomodoro productivity application", "pomodoro productivity extension for vscode", "pomodoro productivity for vscode", "pomodoro productivity vscode extension",
    "pomodoro productivity vscode app", "pomodoro productivity vscode tool", "pomodoro productivity vscode software", "pomodoro productivity vscode application", "vscode pomodoro timer", "vscode pomodoro extension", "vscode pomodoro app",
    "vscode pomodoro tool", "vscode pomodoro software", "vscode pomodoro application", "vscode pomodoro extension for vscode", "vscode pomodoro for vscode", "vscode pomodoro vscode extension", "vscode pomodoro vscode app",
    "vscode pomodoro vscode tool", "vscode pomodoro vscode software", "vscode pomodoro vscode application", "pomodoro productivity vscode extension", "pomodoro productivity vscode app", "pomodoro productivity vscode tool",
    "pomodoro productivity vscode application"
  ],
  "main": "./extension.js",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:focusDev.open",
    "onCommand:focusDev.openSettings"
  ],
  "contributes": {
    "commands": [
      {
        "command": "focusDev.open",
        "title": "FocusDev: Open Timer"
      },
      {
        "command": "focusDev.openSettings",
        "title": "FocusDev: Open Settings"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "FocusDev",
      "properties": {
        "focusDev.workMinutes": {
          "type": "number",
          "default": 25,
          "description": "Work session length in minutes."
        },
        "focusDev.breakMinutes": {
          "type": "number",
          "default": 5,
          "description": "Short break length in minutes."
        },
        "focusDev.longBreakMinutes": {
          "type": "number",
          "default": 15,
          "description": "Long break length in minutes."
        },
        "focusDev.longBreakInterval": {
          "type": "number",
          "default": 4,
          "description": "How many work sessions before a long break."
        },
        "focusDev.autoStartNext": {
          "type": "boolean",
          "default": false,
          "description": "Automatically start the next session when one ends."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "node ./vscode-prepublish.js || true",
    "test": "echo \"No tests\" && exit 0"
  }
}
